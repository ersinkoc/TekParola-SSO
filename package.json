{
  "name": "tekparola",
  "version": "1.0.0",
  "description": "Enterprise Single Sign-On System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/services --testPathPattern=tests/utils",
    "test:integration": "jest --testPathPattern=tests/api",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:migrate:test": "NODE_ENV=test prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "swagger:generate": "ts-node scripts/generate-swagger.ts",
    "generate:secrets": "ts-node scripts/generate-secrets.ts",
    "docker:build": "docker build -t tekparola-sso .",
    "docker:run": "docker run -p 3000:3000 --env-file .env tekparola-sso",
    "docker:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit"
  },
  "keywords": [
    "sso",
    "authentication",
    "enterprise",
    "rbac"
  ],
  "author": "TekParola Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/csv-parse": "^1.1.12",
    "@types/csv-stringify": "^1.4.3",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.0.0",
    "csv-stringify": "^6.5.2",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.1.0",
    "nodemailer": "^6.9.7",
    "qrcode": "^1.5.3",
    "redis": "^4.6.11",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/csurf": "^1.11.5",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.0.40",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.1",
    "@types/nodemailer": "^6.4.14",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
